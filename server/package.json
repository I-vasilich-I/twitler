{
  "name": "twitler-backend",
  "version": "1.0.0",
  "description": "Backend for Twitler(minimalist clone of Twitter)",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "docker": "docker-compose up",
    "docker:wipe": "docker compose up -d -V --build",
    "docker:build": "docker-compose build --no-cache",
    "prisma:migrate": "npx prisma migrate dev --name init",
    "prisma:push": "npx prisma db push",
    "prisma:studio": "npx prisma studio --browser none",
    "start:dev": "nodemon --watch src --watch prisma -L src/index.js",
    "lint": "eslint src --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Oleg Vaskevich",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.5.0",
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^6.14.2",
    "express-winston": "^4.2.0",
    "jsonwebtoken": "^8.5.1",
    "mime-types": "^2.1.35",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.8.0",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "axios": "^1.1.3",
    "eslint": "^8.30.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "nodemon": "^2.0.20",
    "prisma": "^4.5.0"
  }
}
